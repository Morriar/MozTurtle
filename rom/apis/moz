print("loading moz")

os.loadAPI("/rom/apis/moz_core")
core = core

os.loadAPI("/rom/apis/moz_std")
std = moz_std

os.loadAPI("/rom/apis/moz_app")
app = moz_app

os.loadAPI("/rom/apis/moz_map")
map = moz_map

os.loadAPI("/rom/apis/moz_utils")
utils = moz_utils



